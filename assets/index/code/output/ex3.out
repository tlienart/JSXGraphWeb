~~~<div id="brd3" class="jxgbox" style="width:250px;height:200px;"></div>
<script>brd3=JXG.JSXGraph.initBoard("brd3",{"boundingbox":[-12,10,12,-10],"axis":false,"showcopyright":false,"shownavigation":false});a=brd3.create('slider', [[-11,7],[-5,7],[0,3,6]], {"name":"a"});b=brd3.create('slider', [[-11,5],[-5,5],[0,2,6]], {"name":"b"});A=brd3.create('slider', [[1,7],[7,7],[0,3,6]], {"name":"A"});B=brd3.create('slider', [[1,5],[7,5],[0,3,6]], {"name":"B"});delta=brd3.create('slider', [[2.0,-7.0],[6.0,-7.0],[0.0,0.0,3.141592653589793]], {"name":"&delta;"});function f1(t){return (val(A)*sin(((val(a)*t)+val(delta))))};function f2(t){return (val(B)*sin((val(b)*t)))};brd3.create('curve', [function(t){return f1(t);}, function(t){return f2(t);}, 0, 6.283185307179586], {"strokecolor":"#aa2233","strokewidth":3});</script>~~~